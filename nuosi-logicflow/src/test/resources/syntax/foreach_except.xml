<?xml version="1.0" encoding="UTF-8"?>
<logic-flow id="foreach_except"
            name="循环逻辑异常语法"
            desc="循环逻辑异常语法">
    <databus>
    </databus>

    <start id="start" name="开始节点" next="throw_exception"/>

    <action id="throw_exception" name="循环内抛出异常" next="end">
        <foreach iterator="input_list">
        <![CDATA[
            QB.println(INDEX+"==="+ITERATOR);
            if(INDEX==7){
                QB.errorCode("FOREACH_DEFINE_HINT");
            }
        ]]>
        </foreach>
    </action>

    <end id="end" name="结束节点"/>
</logic-flow>