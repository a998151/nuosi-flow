<?xml version="1.0" encoding="UTF-8"?>
<logic-flow id="int_max_limit" name="整数最大值限制" desc="整数最大值限制">
    <!-- 全局域 -->
    <declare>
        <attr id="int_param" type="int" name="整型参数">
            <limitInteger max="7"/>
        </attr>
    </declare>

    <start id="start" name="开始节点" next="printParam">
        <!-- 逻辑入口参数校验 -->
        <var key="int_param"/>
    </start>

    <action id="printParam" name="打印参数" next="end">
        <expression>
        <![CDATA[

        ]]>
        </expression>
    </action>

    <end id="end" name="结束节点">
        <var key="int_param"/>
    </end>
</logic-flow>