<?xml version="1.0" encoding="UTF-8"?>
<logic-flow id="datetime_min_limit" name="日期时间小于值限制" desc="日期时间小于值限制">
    <!-- 全局域 -->
    <declare>
        <attr id="datetime_param" type="datetime" name="日期时间参数">
            <limitDatetime min="2021-07-07 07:07:07"/> <!-- 7:7:7则解析报错 -->
        </attr>
    </declare>

    <start id="start" name="开始节点" next="printParam">
        <!-- 逻辑入口参数校验 -->
        <var key="datetime_param"/>
    </start>

    <action id="printParam" name="打印参数" next="end">
        <expression>
        <![CDATA[

        ]]>
        </expression>
    </action>

    <end id="end" name="结束节点">
        <var key="datetime_param"/>
    </end>
</logic-flow>