<?xml version="1.0" encoding="UTF-8"?>
<logic-flow id="string_more_validate" name="字符串长度大于限制" desc="字符串长度大于限制">
    <!-- 全局域 -->
    <declare>
        <attr id="string_param" type="string" name="字符串参数">
            <validateString more="7"/>
        </attr>
    </declare>

    <start id="start" name="开始节点" next="printParam">
        <!-- 逻辑入口参数校验 -->
        <var key="string_param"/>
    </start>

    <action id="printParam" name="打印参数" next="end">
        <expression>
        <![CDATA[

        ]]>
        </expression>
    </action>

    <end id="end" name="结束节点">
        <var key="string_param"/>
    </end>
</logic-flow>